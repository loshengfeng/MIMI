<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_personal"
    app:startDestination="@id/personalFragment">

    <include app:graph="@navigation/navigation_login" />
    <include app:graph="@navigation/navigation_club_topic" />
    <include app:graph="@navigation/navigation_my_post" />
    <include app:graph="@navigation/navigation_setting" />
    <include app:graph="@navigation/navigation_topup" />

    <fragment
        android:id="@+id/personalFragment"
        android:name="com.dabenxiang.mimi.view.personal.PersonalFragment"
        tools:layout="@layout/fragment_personal">

        <action
            android:id="@+id/action_personalFragment_to_loginFragment"
            app:destination="@id/navigation_login" />

        <action
            android:id="@+id/action_personalFragment_to_myFollowFragment"
            app:destination="@id/myFollowFragment" />

        <action
            android:id="@+id/action_personalFragment_to_myFollowFragmentV2"
            app:destination="@id/myFollowFragmentV2" />

        <action
            android:id="@+id/action_personalFragment_to_orderFragment"
            app:destination="@id/orderFragment" />

        <action
            android:id="@+id/action_personalFragment_to_chatHistoryFragment"
            app:destination="@id/chatHistoryFragment" />

        <action
            android:id="@+id/action_personalFragment_to_myPostFragment"
            app:destination="@id/navigation_my_post" />
        <action
            android:id="@+id/action_to_settingFragment"
            app:destination="@id/navigation_setting" />

        <action
            android:id="@+id/action_personalFragment_to_topupFragment"
            app:destination="@id/navigation_topup" />

        <action
            android:id="@+id/action_to_fanslistFragment"
            app:destination="@id/fansListFragment" />

        <action
            android:id="@+id/action_to_likelistFragment"
            app:destination="@id/likesListFragment" />

        <action
            android:id="@+id/action_to_inviteVipFragment"
            app:destination="@id/inviteVipFragment" />

        <action
            android:id="@+id/action_personalFragment_to_favoriteFragment"
            app:destination="@id/myfavoriteFragment" />
    </fragment>

    <fragment
        android:id="@+id/myFollowFragment"
        android:name="com.dabenxiang.mimi.view.myfollow.MyFollowFragment"
        tools:layout="@layout/fragment_my_follow">

        <action
            android:id="@+id/action_myFollowFragment_to_topicDetailFragment"
            app:destination="@id/navigation_club_topic" />
        <action
            android:id="@+id/action_myFollowFragment_to_navigation_my_post"
            app:destination="@id/navigation_my_post" />
    </fragment>

    <fragment
        android:id="@+id/orderFragment"
        android:name="com.dabenxiang.mimi.view.order.OrderFragment"
        tools:layout="@layout/fragment_order">
        <action
            android:id="@+id/action_orderFragment_to_chatContentFragment"
            app:destination="@id/chatContentFragment" />
        <action
            android:id="@+id/action_orderFragment_to_paymentInfoFragment"
            app:destination="@id/paymentInfoFragment" />
    </fragment>

    <fragment
        android:id="@+id/chatHistoryFragment"
        android:name="com.dabenxiang.mimi.view.chathistory.ChatHistoryFragment"
        tools:layout="@layout/fragment_chat_history">
        <action
            android:id="@+id/action_chatHistoryFragment_to_chatContentFragment"
            app:destination="@id/chatHistoryFragment" />
    </fragment>

    <fragment
        android:id="@+id/chatContentFragment"
        android:name="com.dabenxiang.mimi.view.chatcontent.ChatContentFragment"
        android:label="ChatContentFragment" />

    <fragment
        android:id="@+id/paymentInfoFragment"
        android:name="com.dabenxiang.mimi.view.paymentInfo.PaymentInfoFragment"
        android:label="PaymentInfoFragment" />

    <fragment
        android:id="@+id/fansListFragment"
        android:name="com.dabenxiang.mimi.view.fans.FansListFragment"
        android:label="FansListFragment" />

    <fragment
        android:id="@+id/likesListFragment"
        android:name="com.dabenxiang.mimi.view.like.LikeFragment"
        android:label="LikeFragment" />

    <fragment
        android:id="@+id/inviteVipFragment"
        android:name="com.dabenxiang.mimi.view.invitevip.InviteVipFragment"
        android:label="InviteVipFragment" />
    <fragment
        android:id="@+id/topUpFragment"
        android:name="com.dabenxiang.mimi.view.topup.TopUpFragment"
        android:label="TopUpFragment" />

    <fragment
        android:id="@+id/myfavoriteFragment"
        android:name="com.dabenxiang.mimi.view.favroite.FavoriteFragment"
        android:label="FavoriteFragment" />

    <fragment
        android:id="@+id/myFollowFragmentV2"
        android:name="com.dabenxiang.mimi.view.myfollow.MyFollowFragmentV2"
        android:label="MyFollowFragmentV2" >

        <action
            android:id="@+id/action_to_searchPostFragment"
            app:destination="@id/searchPostFragment" />

        <action
            android:id="@+id/action_to_clubTextFragment"
            app:destination="@id/clubTextFragment" />

        <action
            android:id="@+id/action_to_clubPicFragment"
            app:destination="@id/clubPicFragment" />

        <action
            android:id="@+id/action_to_topicDetailFragment"
            app:destination="@id/navigation_club_topic" />

        <action
            android:id="@+id/action_to_myPostFragment"
            app:destination="@id/navigation_my_post" />

        <action
            android:id="@+id/action_to_loginFragment"
            app:destination="@id/navigation_login" />

        <action
            android:id="@+id/action_to_clipPlayerFragment"
            app:destination="@id/clipPlayerFragment" />

        <action
            android:id="@+id/action_to_postArticleFragment"
            app:destination="@id/postArticleFragment" />

        <action
            android:id="@+id/action_to_postPicFragment"
            app:destination="@id/postPicFragment" />

    </fragment>

    <fragment
        android:id="@+id/searchPostFragment"
        android:name="com.dabenxiang.mimi.view.search.post.SearchPostFragment"
        android:label="SearchPostFragment" >
        <action
            android:id="@+id/action_searchPostFragment_to_clubPicFragment"
            app:destination="@id/clubPicFragment" />
        <action
            android:id="@+id/action_searchPostFragment_to_clubTextFragment"
            app:destination="@id/clubTextFragment" />
        <action
            android:id="@+id/action_searchPostFragment_to_clipPlayerFragment"
            app:destination="@id/clipPlayerFragment" />
        <action
            android:id="@+id/action_searchPostFragment_to_postArticleFragment"
            app:destination="@id/postArticleFragment" />
        <action
            android:id="@+id/action_searchPostFragment_to_postPicFragment"
            app:destination="@id/postPicFragment" />
        <action
            android:id="@+id/action_searchPostFragment_to_postVideoFragment"
            app:destination="@id/postVideoFragment" />
        <action
            android:id="@+id/action_searchPostFragment_to_myPostFragment"
            app:destination="@id/navigation_my_post" />
    </fragment>

    <fragment
        android:id="@+id/clubPicFragment"
        android:name="com.dabenxiang.mimi.view.club.pic.ClubPicFragment"
        android:label="ClubPicFragment" >
        <action
            android:id="@+id/action_to_pictureFragment"
            app:destination="@id/pictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/clipPlayerFragment"
        android:name="com.dabenxiang.mimi.view.player.ui.ClipPlayerFragment"
        android:label="ClipPlayerFragment" />

    <fragment
        android:id="@+id/postArticleFragment"
        android:name="com.dabenxiang.mimi.view.post.article.PostArticleFragment"
        tools:layout="@layout/fragment_post_article">

        <action
            android:id="@+id/action_postArticleFragment_to_searchPostFragment"
            app:destination="@id/searchPostFragment"
            app:popUpTo="@+id/searchPostFragment"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/postPicFragment"
        android:name="com.dabenxiang.mimi.view.post.pic.PostPicFragment"
        android:label="PostPicFragment"
        tools:layout="@layout/fragment_post_pic">
        <action
            android:id="@+id/action_postPicFragment_to_postViewerFragment"
            app:destination="@id/postViewerFragment" />
        <action
            android:id="@+id/action_postPicFragment_to_searchPostFragment"
            app:destination="@id/searchPostFragment"
            app:popUpTo="@+id/searchPostFragment"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/pictureFragment"
        android:name="com.dabenxiang.mimi.view.fullpicture.FullPictureFragment"
        tools:layout="@layout/fragment_full_picture" />

</navigation>