<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_clubFragment"
    app:startDestination="@id/clubFragment">

    <include app:graph="@navigation/navigation_login" />

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.dabenxiang.mimi.view.splash.SplashFragment"
        tools:layout="@layout/fragment_splash">

        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/clubFragment"
        android:name="com.dabenxiang.mimi.view.club.ClubFragment"
        tools:layout="@layout/fragment_club">
    </fragment>

</navigation>