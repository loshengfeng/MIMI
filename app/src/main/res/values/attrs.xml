<resources>
    <declare-styleable name="DateEditText">
        <attr name="dividerCharacter" format="enum">
            <enum name="minus" value="0"/>
        </attr>


        <attr name="dateFormat" format="enum">
            <enum name="ddMMyyyy" value="0"/>
        </attr>

        <attr name="maxDate" format="string"/>
        <attr name="minDate" format="string"/>
        <attr name="autoCorrect" format="boolean"/>
        <attr name="helperTextEnabled" format="boolean"/>
        <attr name="helperTextHighlightedColor" format="color"/>

    </declare-styleable>

    <declare-styleable name="NestedRecyclerView">
        <attr name="allowParentIntercept" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomTabLayout">
        <attr name="exampleString" format="string" />
        <attr name="exampleDimension" format="dimension" />
        <attr name="exampleColor" format="color" />
        <attr name="exampleDrawable" format="color|reference" />
    </declare-styleable>
</resources>